        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.05">
        
        <TextView
            android:id="@+id/txt_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:padding="10dp"
            android:text="Kyaw Min Soe"
            android:textSize="26sp"
            android:textStyle="bold"
            android:textColor="#026095" />      
       
        
        <TextView
            android:id="@+id/txt_add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/img_pic"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/txt_name"
            android:layout_toLeftOf="@+id/img_pic"
            android:padding="10dp"
            android:text="No.(15), 124th street, Mingalar Taung Nyunt, Yangon, Myanmar."
            android:textColor="#FFFFFF"
            android:textSize="11dp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/img_pic"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:onClick="ShowLargeSize"            
            android:padding="10dp"
            android:src="@drawable/my_photo_selector"
            android:tag="Click to View Large Size" />
        
    </RelativeLayout>  
    
